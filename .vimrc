syntax on
filetype plugin on
filetype indent on
set autoread
set encoding=utf8
set background=dark
colorscheme solarized
set number
set laststatus=2
set mouse=a
set mousemodel=popup
set guifont=Inconsolata\ For\ Powerline:h1
set t_Co=2565
hi Normal ctermbg=none
set spelllang=en
set spell
set showmatch 
set shiftwidth=4 
set tabstop=4
set expandtab
set smarttab
set autoindent
set ruler
set ai "Auto indent
set si "Smart indent
set wrap "Wrap lines
set rtp+=/usr/lib/python3.5/site-packages/powerline/bindings/vim

